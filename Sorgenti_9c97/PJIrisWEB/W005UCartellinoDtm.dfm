  object Timbrature: TOracleQuery
    SQL.Strings = (
      'declare'
      'begin'
      '  :errore:=null;'
      '  if :oper = '#39'I'#39' then'
      '    insert into t100_timbrature '
      '      (progressivo,data,ora,verso,flag,causale,rilevatore)'
      '    values'
      
        '      (:progressivo,:data,to_date('#39'01011900'#39'||:ora,'#39'ddmmyyyyhh24' +
        'mi'#39'),:verso,'#39'I'#39',:causale,:rilevatore);'
      '  elsif :oper = '#39'C'#39' then'
      '    if :flag = '#39'O'#39' then'
      
        '      update t100_timbrature set flag = '#39'C'#39' where rowid = :rowid' +
        ';'
      '    else'
      '      delete from t100_timbrature where rowid = :rowid;'
      '    end if;'
      '  elsif :oper = '#39'M'#39' then'
      '    if :flag = '#39'O'#39' and :originale = '#39'N'#39' then'
      
        '      update t100_timbrature set flag = '#39'M'#39' where rowid = :rowid' +
        ';'
      
        '      insert into t100_timbrature (progressivo,data,ora,verso,fl' +
        'ag,causale,rilevatore)'
      
        '        select progressivo,data,to_date('#39'01011900'#39'||:ora,'#39'ddmmyy' +
        'yyhh24mi'#39'),:verso,'#39'I'#39',:causale,:rilevatore'
      '        from t100_timbrature where rowid = :rowid;'
      '    else'
      '      update t100_timbrature set'
      '        ora = to_date('#39'01011900'#39'||:ora,'#39'ddmmyyyyhh24mi'#39'),'
      '        verso = :verso,'
      '        causale = :causale,'
      '        rilevatore = :rilevatore'
      '      where rowid = :rowid;'
      '    end if;'
      '  end if;'
      'exception'
      '  when others then'
      '    :errore:='#39'errore'#39';    '
      'end;')

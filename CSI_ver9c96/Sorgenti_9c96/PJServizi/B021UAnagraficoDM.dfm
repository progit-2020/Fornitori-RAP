  object selCampoT430: TOracleDataSet
    SQL.Strings = (
      'select distinct'
      '  --T030.MATRICOLA,'
      '  --T030.PROGRESSIVO,'
      '  T430.:CAMPO,'
      '  T430.DATADECORRENZA DECORRENZA,'
      '  T430.DATAFINE SCADENZA'
      '  /*'
      '  (select min(DATADECORRENZA) from T430_STORICO T430P'
      
        '   where PROGRESSIVO = T430.PROGRESSIVO and DATADECORRENZA <= T4' +
        '30.DATADECORRENZA'
      '   and nvl(:CAMPO,'#39' '#39') = nvl(T430.:CAMPO,'#39' '#39')'
      
        '   and not exists (select '#39'x'#39' from T430_STORICO where PROGRESSIV' +
        'O = T430.PROGRESSIVO and nvl(:CAMPO,'#39' '#39') <> nvl(T430.:CAMPO,'#39' '#39')' +
        ' and DATADECORRENZA between T430P.DATADECORRENZA and T430.DATADE' +
        'CORRENZA)'
      '  ) DECORRENZA,'
      '  (select max(DATAFINE) from T430_STORICO T430S'
      
        '   where PROGRESSIVO = T430.PROGRESSIVO and DATADECORRENZA >= T4' +
        '30.DATADECORRENZA'
      '   and nvl(:CAMPO,'#39' '#39') = nvl(T430.:CAMPO,'#39' '#39')'
      
        '   and not exists (select '#39'x'#39' from T430_STORICO where PROGRESSIV' +
        'O = T430.PROGRESSIVO and nvl(:CAMPO,'#39' '#39') <> nvl(T430.:CAMPO,'#39' '#39')' +
        ' and DATADECORRENZA between T430.DATADECORRENZA and T430S.DATADE' +
        'CORRENZA)'
      '  ) SCADENZA'
      '  */'
      'from T030_ANAGRAFICO T030, T430_STORICO T430'
      'where T030.PROGRESSIVO = T430.PROGRESSIVO'
      'and T030.PROGRESSIVO = :PROGRESSIVO'

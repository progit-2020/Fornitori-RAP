  object selP442: TOracleDataSet
    SQL.Strings = (
      
        'SELECT P441.DATA_RETRIBUZIONE, P442.COD_CONTRATTO, P442.COD_VOCE' +
        ', SUM(P442.IMPORTO * P216.PERCENTUALE / 100) IMPORTO'
      
        'FROM P442_CEDOLINOVOCI P442, P441_CEDOLINO P441, P216_ACCORPAMEN' +
        'TOVOCI P216,'
      '     T430_STORICO T430, P430_ANAGRAFICO P430'
      'WHERE P442.ID_CEDOLINO = P441.ID_CEDOLINO '
      
        'AND P441.PROGRESSIVO = T430.PROGRESSIVO AND P442.DATA_COMPETENZA' +
        '_A BETWEEN T430.DATADECORRENZA AND T430.DATAFINE'
      
        'AND P441.PROGRESSIVO = P430.PROGRESSIVO AND P442.DATA_COMPETENZA' +
        '_A BETWEEN P430.DECORRENZA AND P430.DECORRENZA_FINE'
      
        'AND LTRIM(RTRIM(P216.COD_TIPOACCORPAMENTOVOCI))||'#39'.'#39'||LTRIM(RTRI' +
        'M(P216.COD_CODICIACCORPAMENTOVOCI)) :Accorpamenti'
      
        'AND P216.COD_CONTRATTO = P442.COD_CONTRATTO AND P216.COD_VOCE = ' +
        'P442.COD_VOCE AND P216.COD_VOCE_SPECIALE = P442.COD_VOCE_SPECIAL' +
        'E'
      
        'AND P442.DATA_COMPETENZA_A BETWEEN P216.DECORRENZA AND P216.DECO' +
        'RRENZA_FINE '
      
        'AND P441.CHIUSO IN (:StatoCedolini) AND P441.TIPO_CEDOLINO <> '#39'R' +
        'P'#39' AND P442.TIPO_RECORD = '#39'M'#39
      
        'AND P442.DATA_COMPETENZA_A BETWEEN :DataInizioCompetenza AND :Da' +
        'taFineCompetenza'
      'AND (:FiltroDipendenti)'
      
        'GROUP BY P441.DATA_RETRIBUZIONE, P442.COD_CONTRATTO, P442.COD_VO' +
        'CE'
      'HAVING SUM(P442.IMPORTO * P216.PERCENTUALE / 100) <> 0'
      
        'ORDER BY P441.DATA_RETRIBUZIONE, P442.COD_CONTRATTO, P442.COD_VO' +
        'CE')

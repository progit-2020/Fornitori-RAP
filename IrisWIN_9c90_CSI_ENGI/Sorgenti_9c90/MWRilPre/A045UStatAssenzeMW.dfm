  object QEstrazionePresenze: TOracleDataSet
    SQL.Strings = (
      
        'SELECT DISTINCT T430.QUALIFICAMINIST CODQUALIF, T470.DESCRIZIONE' +
        ' DESCQUALIF, T430.TIPORAPPORTO, T430.ORARIO ORESETT, '
      '       T100.PROGRESSIVO, T100.DATA, T030.SESSO, '
      
        '       T460.TIPO TIPO_PT, NVL(T460.PIANTA,100) PERC_PT, NVL(T012' +
        '.NUMGIORNI,T011.NUMGIORNI) NUMGIORNISETT, '
      '       T470.PROGRESSIVOQM PROGQUALIF, T470.DEBITOGGQM'
      
        '  FROM T470_QUALIFICAMINIST T470, T011_CALENDARI T011, T012_CALE' +
        'NDINDIVID T012, T100_TIMBRATURE T100, '
      
        '       T030_ANAGRAFICO T030, T460_PARTTIME T460, T430_STORICO T4' +
        '30'
      ' WHERE T030.PROGRESSIVO = :PROG'
      '   AND T030.PROGRESSIVO = T430.PROGRESSIVO'
      '   AND T030.PROGRESSIVO = T100.PROGRESSIVO'
      '   AND T100.FLAG <> '#39'C'#39
      '   AND T470.CODICE = T430.QUALIFICAMINIST'
      '   AND :DATAA BETWEEN T470.DECORRENZA AND T470.DECORRENZA_FINE'
      '   AND T460.CODICE(+) = T430.PARTTIME'
      '   AND T100.DATA BETWEEN :DATADA AND :DATAA'
      
        '   AND LAST_DAY(T100.DATA) BETWEEN T430.DATADECORRENZA AND T430.' +
        'DATAFINE'
      
        '   AND EXISTS (SELECT '#39'X'#39' FROM T430_STORICO WHERE PROGRESSIVO = ' +
        'T430.PROGRESSIVO AND T100.DATA BETWEEN INIZIO AND NVL(FINE,TO_DA' +
        'TE('#39'31123999'#39','#39'DDMMYYYY'#39')))'
      '   AND T011.DATA  = T100.DATA'
      '   AND T011.CODICE = T430.CALENDARIO'
      '   AND T012.PROGRESSIVO(+) = T100.PROGRESSIVO'
      '   AND T012.DATA(+) = T100.DATA'
      '   :FILTRO'
      'UNION'
      
        'SELECT DISTINCT T430.QUALIFICAMINIST CODQUALIF, T470.DESCRIZIONE' +
        ' DESCQUALIF, T430.TIPORAPPORTO, T430.ORARIO ORESETT, '
      '       T040.PROGRESSIVO, T040.DATA, T030.SESSO, '
      
        '       T460.TIPO TIPO_PT, NVL(T460.PIANTA,100) PERC_PT, NVL(T012' +
        '.NUMGIORNI,T011.NUMGIORNI) NUMGIORNISETT, '
      '       T470.PROGRESSIVOQM PROGQUALIF, T470.DEBITOGGQM'
      
        '  FROM T470_QUALIFICAMINIST T470, T011_CALENDARI T011, T012_CALE' +
        'NDINDIVID T012, T040_GIUSTIFICATIVI T040, '
      
        '       T030_ANAGRAFICO T030, T460_PARTTIME T460, T430_STORICO T4' +
        '30'
      ' WHERE T030.PROGRESSIVO = :PROG'
      '   AND T030.PROGRESSIVO = T430.PROGRESSIVO'
      '   AND T030.PROGRESSIVO = T040.PROGRESSIVO'
      '   AND T470.CODICE = T430.QUALIFICAMINIST'
      '   AND :DATAA BETWEEN T470.DECORRENZA AND T470.DECORRENZA_FINE'
      '   AND T460.CODICE(+) = T430.PARTTIME'
      '   AND T040.DATA BETWEEN :DATADA AND :DATAA'
      
        '   AND LAST_DAY(T040.DATA) BETWEEN T430.DATADECORRENZA AND T430.' +
        'DATAFINE'
      
        '   AND EXISTS (SELECT '#39'X'#39' FROM T430_STORICO WHERE PROGRESSIVO = ' +
        'T430.PROGRESSIVO AND T040.DATA BETWEEN INIZIO AND NVL(FINE,TO_DA' +
        'TE('#39'31123999'#39','#39'DDMMYYYY'#39')))'
      '   AND T011.DATA  = T040.DATA'
      '   AND T011.CODICE = T430.CALENDARIO'
      '   AND T012.PROGRESSIVO(+) = T040.PROGRESSIVO'
      '   AND T012.DATA(+) = T040.DATA'
      '   :FILTRO'
      'ORDER BY PROGRESSIVO, DATA'
      '')
    Optimize = False
    Variables.Data = {
      04000000040000000E0000003A004400410054004100440041000C0000000000
      0000000000000C0000003A00440041005400410041000C000000000000000000
      00000A0000003A00500052004F0047000300000000000000000000000E000000
      3A00460049004C00540052004F00010000000000000000000000}
    Left = 148
    Top = 72
  end
  object selT043: TOracleDataSet
    SQL.Strings = (
      'select T470.PROGRESSIVOQM, T470.CODICE, T470.DESCRIZIONE,'
      '       T.COD_CODICIACCORPAMENTOASS, T030.SESSO, '
      '       SUM(FRUITO) NGIORNI, COUNT(DISTINCT T.PROGRESSIVO) NDIP'
      
        '  from T030_ANAGRAFICO T030, T430_STORICO T430, t043_assenzemens' +
        'ili t, t470_qualificaminist t470'
      '  WHERE T.PROGRESSIVO = T030.PROGRESSIVO'
      '    AND T030.PROGRESSIVO = T430.PROGRESSIVO'
      
        '    AND EXISTS (SELECT /*+ UNNEST */ '#39'X'#39' FROM :C700SelAnagrafe a' +
        'nd T030.PROGRESSIVO = T.PROGRESSIVO)'
      
        '    AND T.DATA_FINE_PERIODO BETWEEN T430.DATADECORRENZA AND T430' +
        '.DATAFINE'
      '    AND T430.QUALIFICAMINIST = T470.CODICE'
      
        '    AND :DataLavoro BETWEEN T470.DECORRENZA AND T470.DECORRENZA_' +
        'FINE'
      '    AND T.DATA_FINE_PERIODO BETWEEN :DATAI AND :DataLavoro'
      '        :FILTRO'
      
        'GROUP BY T470.PROGRESSIVOQM, T470.CODICE, T470.DESCRIZIONE, T.CO' +
        'D_CODICIACCORPAMENTOASS, SESSO')
